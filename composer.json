{
    "name": "jmarchalonis/cakephp-cors",
    "description": "A CakePHP (4.x) plugin for activate cors domain in your application",
    "type": "cakephp-plugin",
    "require": {
        "php": "^7.2",
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Cors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cors\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
